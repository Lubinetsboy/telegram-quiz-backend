# Telegram Bot Token от @BotFather
TELEGRAM_BOT_TOKEN=your_bot_token_here

# База данных SQLite (по умолчанию создастся в backend/database.sqlite)
DATABASE_URL=sqlite:./database.sqlite

# Telegram ID админов через запятую (можно узнать у @userinfobot)
ADMIN_IDS=123456789

# Порт для Express сервера
PORT=3000

# URL веб-приложения (для локального запуска используйте ngrok или другой туннель)
# Пример с ngrok: https://abc123.ngrok.io
# После запуска ngrok скопируйте HTTPS URL сюда
WEB_APP_URL=https://your-ngrok-url.ngrok.io
